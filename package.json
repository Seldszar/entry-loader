    "@commitlint/config-conventional": "^11.0.0",
    "ava": "^3.13.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "memfs": "^3.2.0",
    "webpack": "^4.44.2",
    "xo": "^0.33.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
